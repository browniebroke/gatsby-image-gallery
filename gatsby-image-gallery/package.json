{
  "name": "@browniebroke/gatsby-image-gallery",
  "version": "0.0.0",
  "description": "Very basic gallery grid based on gatsby-image.",
  "author": "browniebroke",
  "license": "MIT",
  "repository": "browniebroke/gatsby-image-gallery",
  "homepage": "https://gatsby-image-gallery.netlify.app",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/gatsby-image-gallery.esm.js",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14.15"
  },
  "scripts": {
    "start": "dts watch",
    "build": "dts build",
    "test": "dts test --passWithNoTests",
    "watch": "dts test --watchAll",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "gatsby-plugin-image": "^2.0",
    "react": ">=16",
    "styled-components": ">= 5.0"
  },
  "dependencies": {
    "react-image-lightbox": "github:browniebroke/react-image-lightbox",
    "styled-components": ">= 5.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "7.0.8",
    "@types/jest": "27.5.0",
    "@types/react": "17.0.44",
    "@types/react-dom": "17.0.16",
    "@types/react-test-renderer": "17.0.2",
    "@types/styled-components": "5.1.25",
    "babel-jest": "28.0.1",
    "dts-cli": "1.5.1",
    "gatsby-plugin-image": "2.13.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-test-renderer": "17.0.2",
    "size-limit": "7.0.8",
    "ts-jest": "27.1.4",
    "tslib": "2.4.0",
    "typescript": "4.6.4",
    "typescript-plugin-css-modules": "3.4.0"
  },
  "size-limit": [
    {
      "path": "dist/gatsby-image-gallery.cjs.production.min.js",
      "limit": "60 KB"
    },
    {
      "path": "dist/gatsby-image-gallery.esm.js",
      "limit": "60 KB"
    }
  ]
}
