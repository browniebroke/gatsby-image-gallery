{
  "name": "@browniebroke/gatsby-image-gallery",
  "version": "0.0.0",
  "description": "Very basic gallery grid based on gatsby-image.",
  "author": "browniebroke",
  "license": "MIT",
  "repository": "browniebroke/gatsby-image-gallery",
  "homepage": "https://gatsby-image-gallery.netlify.app",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/gatsby-image-gallery.esm.js",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "dts watch",
    "build": "dts build",
    "test": "dts test --passWithNoTests",
    "test:update": "dts test -u --passWithNoTests",
    "watch": "dts test --watchAll",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "gatsby-plugin-image": "^3.0.0",
    "react": ">=16",
    "styled-components": ">= 5.0"
  },
  "dependencies": {
    "react-image-lightbox": "github:browniebroke/react-image-lightbox",
    "styled-components": ">= 5.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "11.2.0",
    "@types/jest": "29.5.14",
    "@types/react": "18.3.22",
    "@types/react-dom": "18.3.7",
    "@types/react-test-renderer": "18.3.1",
    "@types/styled-components": "5.1.34",
    "babel-jest": "29.7.0",
    "dts-cli": "2.0.5",
    "gatsby-plugin-image": "3.14.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-test-renderer": "18.3.1",
    "size-limit": "11.2.0",
    "ts-jest": "29.3.4",
    "tslib": "2.8.1",
    "typescript": "5.8.3",
    "typescript-plugin-css-modules": "5.1.0"
  },
  "size-limit": [
    {
      "path": "dist/gatsby-image-gallery.cjs.production.min.js",
      "limit": "60 KB"
    },
    {
      "path": "dist/gatsby-image-gallery.esm.js",
      "limit": "60 KB"
    }
  ]
}
