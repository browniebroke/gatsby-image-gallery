{
  "name": "@browniebroke/gatsby-image-gallery",
  "version": "0.0.0",
  "description": "Very basic gallery grid based on gatsby-image.",
  "author": "browniebroke",
  "license": "MIT",
  "repository": "browniebroke/gatsby-image-gallery",
  "homepage": "https://gatsby-image-gallery.netlify.app",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/gatsby-image-gallery.esm.js",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12.13"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "gatsby-plugin-image": "^1.0",
    "react": ">=16",
    "styled-components": ">= 5.0"
  },
  "dependencies": {
    "react-image-lightbox": "5.1.1",
    "styled-components": "5.2.1"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "4.10.1",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.2",
    "@types/react-test-renderer": "17.0.1",
    "@types/styled-components": "5.1.8",
    "babel-jest": "^26.6.3",
    "gatsby-plugin-image": "1.0.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-test-renderer": "17.0.1",
    "size-limit": "4.10.1",
    "tsdx": "0.14.1",
    "tslib": "2.1.0",
    "typescript": "4.2.3",
    "typescript-plugin-css-modules": "3.2.0"
  },
  "size-limit": [
    {
      "path": "dist/gatsby-image-gallery.cjs.production.min.js",
      "limit": "60 KB"
    },
    {
      "path": "dist/gatsby-image-gallery.esm.js",
      "limit": "60 KB"
    }
  ]
}
