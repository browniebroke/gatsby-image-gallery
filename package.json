{
  "private": true,
  "workspaces": [
    "gatsby-image-gallery",
    "example"
  ],
  "devDependencies": {
    "prettier": "2.2.1"
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "prettier --check ."
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "arrowParens": "always"
  },
  "renovate": {
    "extends": [
      "github>browniebroke/renovate-configs:js-lib"
    ]
  }
}
