{
  "name": "@browniebroke/gatsby-image-gallery",
  "description": "Very basic gallery grid based on gatsby-image.",
  "version": "0.3.0",
  "author": "Bruno Alla <alla.brunoo@gmail.com>",
  "license": "MIT",
  "main": "./dist/index.js",
  "scripts": {
    "build-dev": "webpack --mode=development",
    "build": "webpack --mode=production",
    "test": "jest",
    "bundlesize": "bundlesize",
    "prebundlesize": "yarn build",
    "lint": "prettier --write \"**/*.+(js|jsx|json|css|scss)\""
  },
  "bundlesize": [
    {
      "path": "./dist/index.js",
      "maxSize": "3 kB"
    }
  ],
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/browniebroke/gatsby-image-gallery.git"
  },
  "keywords": [
    "gallery",
    "gatsby",
    "gatsby-image",
    "image"
  ],
  "bugs": {
    "url": "https://github.com/browniebroke/gatsby-image-gallery/issues"
  },
  "homepage": "https://github.com/browniebroke/gatsby-image-gallery#readme",
  "devDependencies": {
    "@babel/core": "7.4.4",
    "@babel/preset-env": "7.4.4",
    "@babel/preset-react": "7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.6",
    "bundlesize": "0.17.2",
    "css-loader": "2.1.1",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.13.0",
    "gatsby-image": "2.0.36",
    "jest": "24.8.0",
    "prettier": "1.17.1",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-test-renderer": "16.8.6",
    "semantic-release": "15.13.12",
    "style-loader": "0.23.1",
    "webpack": "4.32.0",
    "webpack-cli": "3.3.2",
    "webpack-node-externals": "1.7.2"
  },
  "dependencies": {
    "react-image-lightbox": "5.1.0"
  },
  "peerDependencies": {
    "gatsby-image": ">= 2.0",
    "prop-types": ">= 15.6",
    "react": ">= 16.0"
  }
}
