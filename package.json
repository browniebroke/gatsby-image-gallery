{
  "name": "@browniebroke/gatsby-image-gallery",
  "description": "Very basic gallery grid based on gatsby-image.",
  "version": "0.3.0",
  "author": "Bruno Alla <alla.brunoo@gmail.com>",
  "license": "MIT",
  "main": "./dist/index.js",
  "scripts": {
    "build-dev": "webpack --mode=development",
    "build": "webpack --mode=production",
    "test": "yarn run build",
    "bundlesize": "bundlesize",
    "prebundlesize": "yarn build",
    "lint": "prettier --write \"**/*.+(js|jsx|json|css|scss)\""
  },
  "bundlesize": [
    {
      "path": "./dist/index.js",
      "maxSize": "3 kB"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/browniebroke/gatsby-image-gallery.git"
  },
  "keywords": [
    "gallery",
    "gatsby",
    "gatsby-image",
    "image"
  ],
  "bugs": {
    "url": "https://github.com/browniebroke/gatsby-image-gallery/issues"
  },
  "homepage": "https://github.com/browniebroke/gatsby-image-gallery#readme",
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.2.3",
    "@babel/preset-react": "7.0.0",
    "babel-loader": "8.0.5",
    "bundlesize": "0.17.0",
    "css-loader": "2.1.0",
    "eslint": "5.12.0",
    "eslint-config-prettier": "3.4.0",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.3",
    "gatsby-image": "2.0.26",
    "prettier": "1.15.3",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "semantic-release": "15.13.3",
    "style-loader": "0.23.1",
    "webpack": "4.28.4",
    "webpack-cli": "3.2.1",
    "webpack-node-externals": "1.7.2"
  },
  "dependencies": {
    "react-image-lightbox": "5.1.0"
  },
  "peerDependencies": {
    "gatsby-image": ">= 2.0",
    "react": ">= 16.0"
  }
}
